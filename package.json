{
  "name": "fe-supervisor",
  "version": "0.0.14",
  "description": "The frontend monitor project",
  "main": "dist/fe-supervisor.js",
  "types": "dist/fe-supervisor.d.ts",
  "scripts": {
    "dev": "supervisor -w demo,dist demo/server.js",
    "build": "npm run lint && npm run build_branch && npm run build_rollup && npm run build_copy && npm run build_version && npm run build_git",
    "build:beta": "npm run build_branch && npm run build_version_beta && npm run build_git",
    "build_test": "karma start --single-run",
    "build_copy": "node build/copy",
    "build_branch": "node build/branch",
    "build_version": "node build/version",
    "build_version_beta": "node build/version beta",
    "build_git": "node build/git.js master",
    "build_rollup": "tsc --build tsconfig.json && rollup -c build/rollup.config.js",
    "lint": "eslint --ext .ts src/"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "performance",
    "error",
    "frontend",
    "monitor",
    "typescript"
  ],
  "author": "bobby.li",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/plugin-transform-destructuring": "^7.4.4",
    "@babel/plugin-transform-function-name": "^7.4.4",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-property-mutators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-transform-shorthand-properties": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "core-js": "^3.1.3",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "ora": "^3.4.0",
    "pre-commit": "^1.2.2",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "shelljs": "^0.8.3",
    "simple-git": "^1.113.0",
    "supervisor": "^0.12.0",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
  }
}
