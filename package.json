{
  "name": "fe-supervisor",
  "version": "0.1.5",
  "description": "The frontend monitor project",
  "main": "dist/fe-supervisor.js",
  "module": "dist/fe-supervisor.esm.js",
  "types": "dist/fe-supervisor.d.ts",
  "scripts": {
    "dev": "supervisor -w demo/server/helper/* demo/server/app.js",
    "release": "npm run build && npm run build_git",
    "build": "npm run build_branch && npm run lint && npm run build_test && npm run build_version && npm run build_rollup && npm run build_copy && npm run build_git",
    "build:beta": "npm run build_branch && npm run lint && npm run build_test && npm run build_version_beta && npm run build_rollup_beta && npm run build_copy && npm run build_git",
    "build_test": "npm run test_build",
    "build_copy": "node build/copy",
    "build_branch": "./build/branch.sh",
    "build_version": "node build/version",
    "build_version_beta": "node build/version beta",
    "build_git": "./build/git.sh",
    "build_rollup": "rollup -c build/rollup.config.js",
    "build_rollup_beta": "rollup -c build/rollup.config.js --environment beta",
    "lint": "eslint --ext .ts src/",
    "test_build": "karma start --single-run && npm run test_node",
    "test_node": "nyc mocha --opts mocha.opts",
    "test_headless": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test_browser": "karma start --browsers Chrome"
  },
  "pre-commit": [
    "lint",
    "test_node",
    "test_headless"
  ],
  "keywords": [
    "performance",
    "error",
    "frontend",
    "monitor",
    "typescript"
  ],
  "author": "bobby.li",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.5",
    "peeler-js": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/plugin-transform-destructuring": "^7.4.4",
    "@babel/plugin-transform-function-name": "^7.4.4",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-property-mutators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-transform-shorthand-properties": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "core-js": "^3.1.3",
    "del": "^4.1.1",
    "eslint": "^5.16.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "^4.1.0",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ora": "^3.4.0",
    "pre-commit": "^1.2.2",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "supervisor": "^0.12.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1",
    "webpack": "^4.35.0"
  }
}
